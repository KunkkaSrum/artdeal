<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yixing.user.dao.RoleMapper">
    <select id="getByUserName" parameterType="java.lang.String" resultType="com.yixing.user.entity.SysUser">
        SELECT * FROM user
        where username = #{username}
    </select>
</mapper>
